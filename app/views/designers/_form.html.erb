<%= simple_form_for(@designer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name_first %>
    <%= f.input :name_last %>
    <%= f.input :bio %>
    <%= f.input :active %>
    <%= f.input :affiliate_list %>
    <%= f.input :origin %>
    <%= f.input :production_date %>
    <%= f.input :website %>
    <%= f.input :filemaker_id %>
    <%= f.input :listed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
