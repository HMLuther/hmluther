<%= simple_form_for(@image) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :item_id %>
    <%= f.input :filemaker_id %>
    <%= f.input :active %>
    <%= f.input :primary %>
    <%= f.input :banner %>
    <%= f.input :thumb %>
    <%= f.input :webcomp %>
    <%= f.input :image_type %>
    <%= f.input :position %>
    <%= f.input :url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
